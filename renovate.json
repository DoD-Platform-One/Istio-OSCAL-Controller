{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "kubernetes": {
    "managerFilePatterns": [
      "/chart/.+\\.yaml$/"
    ]
  },
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "registryUrls": [
        "https://registry1.dso.mil"
      ],
      "enabled": true,
      "matchPackageNames": [
        "/ironbank/tetrate/oscal-controller/"
      ]
    }
  ]
}
